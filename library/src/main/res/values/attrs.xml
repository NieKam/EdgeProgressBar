<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="EdgeProgressBar">

    <attr name="progress_color" format="color" />
    <attr name="tint_color" format="color" />
    <attr name="line_width" format="dimension" />
    <attr name="indeterminate" format="boolean" />
    <attr name="max" format="integer" />
    <attr name="start_progress" format="float" />
    <attr name="progress_anim_duration" format="integer" />
    <attr name="indeterminate_type" format="enum">
      <enum name="zigzag" value="0" />
      <enum name="snake" value="1" />
      <enum name="glow" value="2" />
      <enum name="dash" value="3" />
    </attr>

  </declare-styleable>

</resources>